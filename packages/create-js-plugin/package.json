{
  "name": "md-create-js-plugin",
  "version": "1.0.0",
  "description": "",
  "author": "SunLxy <1011771396@qq.com>",
  "homepage": "",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "esm",
    "dist"
  ],
  "scripts": {
    "build": "npm run disable:build && npm run bundle && npm run bundle:mini",
    "disable:build": "tsbb build --disable-babel",
    "watch": "tsbb watch  --disable-babel",
    "watch:bundle": "ncc watch src/index.ts --target node --filename index",
    "bundle": "ncc build src/index.ts --target node --filename index",
    "bundle:mini": "ncc build src/index.ts --target node --filename index --minify"
  },
  "dependencies": {
    "@babel/standalone": "~7.17.6",
    "fs-extra": "~10.1.0",
    "remark-parse": "~10.0.1",
    "rehype-attr": "~2.0.8",
    "rehype-autolink-headings": "~6.1.1",
    "rehype-prism-plus": "~1.3.2",
    "rehype-raw": "~6.1.1",
    "rehype-rewrite": "~3.0.6",
    "rehype-slug": "~5.0.1",
    "remark-gfm": "~3.0.1",
    "unified": "~10.0.0",
    "vfile": "~5.0.0",
    "remark-rehype": "~10.1.0"
  },
  "devDependencies": {
    "@uiw/copy-to-clipboard": "^1.0.12",
    "@types/hast": "^2.3.4",
    "@types/babel__standalone": "^7.1.4",
    "@types/fs-extra": "9.0.13",
    "@kkt/ncc": "~1.0.8"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
